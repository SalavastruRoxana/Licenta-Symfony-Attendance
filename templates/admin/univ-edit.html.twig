{% extends "layouts/admin-grid.html.twig" %}
{% block title %} Pagina Institutiei {% endblock %}


{% block content_centru %}
    <h1>Pagina Institutiei</h1>

    {{ form_start(form) }}
    <div class="my-custom-class-for-errors">
        {{ form_errors(form) }}
    </div>

    <div class="col">
        {{ form_row(form.numeleUniversitatii) }}
    </div>
    <div class="col" id="some-custom-id">
        {{ form_row(form.numeleFacultatii) }}
    </div>
    <div class="col" id="some-custom-id">
        {{ form_row(form.email) }}
    </div>
    <div class="col" id="some-custom-id">
        {{ form_row(form.nrTel) }}
    </div>
    <div class="col" id="some-custom-id">
        {{ form_row(form.adresa) }}
    </div>

    {{ form_end(form) }}
{% endblock %}

